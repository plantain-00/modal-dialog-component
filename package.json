{
  "name": "modal-dialog-component",
  "version": "1.0.0",
  "description": "A vuejs, reactjs modal dialog component.",
  "main": "index.js",
  "scripts": {
    "cleanRev": "rimraf demo/**/index.bundle-*.js demo/*.bundle-*.css",
    "clean": "rimraf dist/",
    "file2variable": "file2variable-cli src/vue.template.html -o src/vue-variables.ts --html-minify --base src",
    "tsc": "tsc -p src/ && tsc -p demo/",
    "lessc": "lessc src/modal-dialog.less > dist/modal-dialog.css",
    "cleancss": "cleancss -o dist/modal-dialog.min.css dist/modal-dialog.css",
    "cleancssDemo": "cleancss -o demo/index.bundle.css dist/modal-dialog.min.css ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css",
    "webpack": "webpack --config demo/webpack.config.js",
    "revStatic": "rev-static --config demo/rev-static.config.js",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" \"spec/**/*.ts\" \"demo/**/*.ts\" \"demo/**/*.tsx\"",
    "stylelint": "stylelint \"src/**/*.less\"",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "test": "tsc -p spec && karma start spec/karma.config.js",
    "build": "npm run cleanRev && npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run cleancssDemo && npm run webpack && npm run revStatic",
    "lint": "npm run tslint && npm run stylelint && npm run standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/modal-dialog-component.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/modal-dialog-component/issues"
  },
  "homepage": "https://github.com/plantain-00/modal-dialog-component#readme",
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/react": "15.0.34",
    "@types/react-dom": "15.5.1",
    "clean-css-cli": "4.1.6",
    "file2variable-cli": "1.4.0",
    "github-fork-ribbon-css": "0.2.1",
    "jasmine": "2.6.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-webpack": "2.0.3",
    "less": "2.7.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "rev-static": "2.7.1",
    "rimraf": "2.6.1",
    "standard": "10.0.2",
    "stylelint": "7.12.0",
    "stylelint-config-standard": "16.0.0",
    "tslint": "5.4.3",
    "typescript": "2.4.0",
    "vue": "2.3.4",
    "vue-class-component": "5.0.2",
    "webpack": "3.0.0"
  },
  "dependencies": {
    "tslib": "1"
  }
}
